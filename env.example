# Configuration template for SenVoice SDK
# Copy this file to .env and fill with your actual values

# RunPod API Key (required for RunPod endpoints)
RUNPOD_API_KEY=your_runpod_api_key_here

# RunPod Endpoint IDs (cloud endpoints with authentication)
# Unified models supporting both French and Wolof
TTS_ENDPOINT=your_unified_tts_endpoint_id
ASR_ENDPOINT=your_unified_asr_endpoint_id

# Local Endpoint URLs (local endpoints without authentication)
# Unified models supporting both French and Wolof
TTS_LOCAL_ENDPOINT=http://localhost:8001
ASR_LOCAL_ENDPOINT=http://localhost:8002
