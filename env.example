# Configuration template for SenVoice SDK
# Copy this file to .env and fill with your actual values

# RunPod API Key (required for RunPod endpoints)
RUNPOD_API_KEY=your_runpod_api_key_here

# RunPod Endpoint IDs (cloud endpoints with authentication)
TTS_FR_ENDPOINT=your_tts_french_endpoint_id
TTS_WO_ENDPOINT=your_tts_wolof_endpoint_id
STT_FR_ENDPOINT=your_stt_french_endpoint_id
STT_WO_ENDPOINT=your_stt_wolof_endpoint_id

# Local Endpoint URLs (local endpoints without authentication)
TTS_FR_LOCAL_ENDPOINT=http://localhost:8001
TTS_WO_LOCAL_ENDPOINT=http://localhost:8002
STT_FR_LOCAL_ENDPOINT=http://localhost:8003
STT_WO_LOCAL_ENDPOINT=http://localhost:8004
